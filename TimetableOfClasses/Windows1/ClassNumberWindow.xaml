<Window x:Class="TimetableOfClasses.Windows1.ClassNumberWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimetableOfClasses.Windows1"
        mc:Ignorable="d"
        Height="120" Width="340.333"
        KeyUp="Window_KeyUp"
        ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal" Margin="0, 10, 0, 0">
                <Label>Номер кабинета</Label>
                <TextBox x:Name="ClassNumberTextBox" Style="{StaticResource TextBox}" Width="195" Margin="15,0,0,0"/>
            </StackPanel>

            <Button x:Name="SaveButton" Content="Сохранить"  Style="{StaticResource Button}" Margin="0,10,0, 0" Click="SaveButton_Click"  />
        </StackPanel>
    </Grid>
</Window>
